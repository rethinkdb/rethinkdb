rule test_all
{
   local all_rules = ;

   for local fileb in [ glob *.cpp ]
   {
      all_rules += [ run $(fileb) ] ;
   }

   return $(all_rules) ;
}

test-suite boost-geometry-index-rtree-generated : [ test_all r ] ;

