    THREADS=`$CC -v 2>&1 | sed -n 's/^Thread model: //p'`
    if test -z "$THREADS"; then
      THREADS=no
    fi
    { echo "$as_me:$LINENO: result: $THREADS" >&5
echo "${ECHO_T}$THREADS" >&6; }
fi


