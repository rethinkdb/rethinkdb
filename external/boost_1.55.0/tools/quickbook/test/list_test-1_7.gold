                </listitem>
                <listitem>
                  <simpara>
                    H
                  </simpara>
                </listitem>
              </itemizedlist>
            </simpara>
          </listitem>
        </itemizedlist>
      </simpara>
    </listitem>
  </orderedlist>
  <para>
    Three level list:
  </para>
  <orderedlist>
    <listitem>
      <simpara>
        A
        <itemizedlist>
          <listitem>
            <simpara>
              A
            </simpara>
          </listitem>
          <listitem>
            <simpara>
              B
